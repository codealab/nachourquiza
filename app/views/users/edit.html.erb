<% provide(:title, "Editar #{@user.name}") %>

<% if !@user.admin? %>
	<div class="boton_der">
		<div class="top_buttons">
			<%= link_factory( :_path => users_path, :_text=> 'Todos los Usuarios', :_button => true, :_class=> 'btn btn-info' ) %>
		</div>
		<h1>Editar <%= @user.name %><span class="line"></span></h1>
	</div>
<% end %>
<%= render 'users/form' %>